#include "../lib/keyboard.h"
#include "../lib/random.h"
#include "../lib/display.h"
#include "maze.h"

import u8 trap();

export u8 main() {
    u8 y;
    u8 n = (u8)9;
    srand((u16)12520);
    maze_init(n, n);
    maze_dfs((u8)0, (u8)0);
    trap();
    y = (u8)0;
    while (y < n) {
        maze_print((u8)4, y, NORTH);
        display_clear();
        y += (u8)1;
    }
    trap();
}
