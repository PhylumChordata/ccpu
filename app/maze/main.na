#include "../lib/keyboard.h"
#include "../lib/random.h"
#include "../lib/display.h"
#include "maze.h"

import u8 trap();

export u8 main() {
    u8 y;
    srand((u16)12512);
    maze_init((u8)7, (u8)7);
    maze_dfs(0, 0);
    trap();
    y = (u8)0;
    while (y < (u8)7) {
        maze_print((u8)4, y, NORTH);
        display_clear();
        y += (u8)1;
    }
    trap();
}
